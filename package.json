{
  "name": "vibe-commander",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#features/*": "./src/features/*",
    "#bindings/*": "./src/bindings/*",
    "#core/*": "./src/core/*",
    "#lib/*": "./src/lib/*",
    "#stores/*": "./src/stores/*",
    "#testing/*": "./src/testing/*",
    "#vitest-browser-solid/*": "./vitest-browser-solid/*.ts",
    "#tauri-bindings/*": "./tauri-bindings/*.ts"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsgo && vite build",
    "test": "vitest --run",
    "test:watch": "bun vitest",
    "test:unit": "bun vitest run --project unit",
    "test:property": "bun vitest run --project property",
    "test:browser": "bun vitest run --project browser",
    "test:browser:headed": "HEADED=true bun vitest run --project browser",
    "test:e2e": "wdio run e2e/wdio.conf.ts",
    "tauri": "tauri",
    "typecheck": "tsc",
    "lint": "oxlint --type-aware --type-check --config oxlint.json src",
    "lint:fix": "oxlint --type-aware --type-check --config oxlint.json --fix src",
    "lint:eslint": "eslint src/",
    "lint:eslint:fix": "eslint --fix src/",
    "fmt": "oxfmt --config oxfmt.json --write",
    "fmt:check": "oxfmt --config oxfmt.json --check",
    "upgrade-deps": "bunx npm-check-updates -i"
  },
  "dependencies": {
    "@tauri-apps/api": "2.9.1",
    "@tauri-apps/plugin-opener": "2.5.3",
    "@tauri-apps/plugin-window-state": "~2",
    "solid-js": "1.9.11"
  },
  "devDependencies": {
    "@solidjs/testing-library": "0.8.10",
    "@tailwindcss/vite": "4.1.18",
    "@tauri-apps/cli": "2.9.6",
    "@types/bun": "1.3.7",
    "@types/mocha": "10.0.10",
    "@typescript-eslint/parser": "^8.54.0",
    "@typescript/native-preview": "7.0.0-dev.20260128.1",
    "@vitest/browser-webdriverio": "4.0.18",
    "@wdio/cli": "9.23.2",
    "@wdio/globals": "9.23.0",
    "@wdio/local-runner": "9.23.2",
    "@wdio/mocha-framework": "9.23.2",
    "@wdio/spec-reporter": "9.23.2",
    "eslint": "9.39.2",
    "eslint-plugin-solid": "0.14.5",
    "fast-check": "4.5.3",
    "oxfmt": "0.27.0",
    "oxlint": "1.42.0",
    "oxlint-tsgolint": "0.11.3",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-solid": "2.11.10",
    "vitest": "4.0.18"
  }
}
